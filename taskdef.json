{
    "family": "flask-ecs-task",
    "containerDefinitions": [
        {
            "name": "flask-container",
            "image": "flask-ecs-cicd",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "flask",
                    "containerPort": 5000,
                    "hostPort": 5000,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "executionRoleArn": "arn:aws:iam::226030785019:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}
