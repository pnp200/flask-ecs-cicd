{
    "family": "flask-ecs-task",
    "containerDefinitions": [
        {
            "name": "flask-container",
            "image": "226030785019.dkr.ecr.ap-southeast-1.amazonaws.com/flask-ecs-cicd:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "flask",
                    "containerPort": 5000,
                    "hostPort": 5000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "systemControls": []
        }
    ],
    "executionRoleArn": "arn:aws:iam::226030785019:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "enableFaultInjection": false
}
